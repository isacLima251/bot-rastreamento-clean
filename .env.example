# Exemplo de configurações de ambiente
# Copie este arquivo para ".env" e preencha com seus valores

# Chave usada para assinar JWTs
JWT_SECRET=

# Chave de acesso à API do Site Rastreio
SITERASTREIO_API_KEY=

# Chave para autenticar webhooks da Ticto
TICTO_SECRET=

# Porta em que o servidor Express irá rodar (opcional)
PORT=3000


# Caminho opcional para o arquivo SQLite
DB_PATH=

# API Key do Resend para envio de emails automáticos
RESEND_API_KEY=

# URL base do aplicativo (usada nos emails de boas-vindas)
APP_URL=http://localhost:3000
